<main>
<sidebar>
   
</sidebar>

<blog>
    <blog-head>
        
    </blog-head>
    <blog-body>
        <div class="itme-box">
            <% @items.each do |item| %>
            <div class="itme">
                <%= item.title %>
                 <%= link_to user_item_path(item.id) do %>
                <%= attachment_image_tag item, :image, :fill, 100, 100 %>
                <% end %>
                <%= item.shop_name %>
                <%= item.place %>
                <%= item.commodity %>
                <%= item.price %>
                <%= item.body %>
            </div>
            <% end %>
        </div>
    </blog-body>
</blog>
</main>
