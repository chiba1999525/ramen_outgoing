<title></title>
  <h2></h2> 
    <%= ("#{@another_entry.user.email}さんとのメッセージ") %>

 <% @messages.each do |m| %>
   <% if m.user_id == current_user.id %>
     <mycomment></mycomment>
      <p></p> 
        <%= m.body %>
    <% else %>
   <fukidasi></fukidasi>
     <faceicon></faceicon>
        <%= image_tag m.user.email %>
     <chatting></chatting>
       <says></says>
         <p></p> 
          <%= m.body %>
          
          
          <chat-form-box></chat-form-box>
       <%= form_for @message do |f| %>
        <chat-form-group></chat-form-group>
        <% = f.text_field :body %>
        <% = f.hidden_field :room_id, :value => @room.id %>
        <%= f.submit "送信する" %>